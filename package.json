{
  "name": "react-project-template",
  "version": "1.0.0",
  "description": "A React project tempalte.",
  "main": "index.js",
  "repository": "https://iknowhtml.github.io/react-project-template.git",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --progress",
    "start:staging": "NODE_ENV=staging webpack-dev-server --progress",
    "start:production": "NODE_ENV=production webpack-dev-server --progress",
    "build": "yarn build:dev",
    "build:staging": "NODE_ENV=staging webpack -p --progress --display-error-details",
    "build:production": "NODE_ENV=production webpack -p --progress --display-error-details",
    "lint": "eslint --fix src/",
    "clean:modules": "rm -rf node_modules",
    "clean:build": "rm -rf dist"
  },
  "author": "Aki Gao",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.29.0",
    "html-webpack-template": "^5.6.0",
    "redux-logger": "^3.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "numeral": "^2.0.6",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-form": "^7.0.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  }
}
